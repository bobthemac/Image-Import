<Window x:Class="Image_Import.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Image_Import"
        mc:Ignorable="d"
        Title="Image Importer" Height="350" Width="525">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="117,14,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.517,-1.544"/>
        <Label x:Name="label" Content="Select Flash Drive" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Import" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="227" Click="button_Click"/>

    </Grid>
</Window>
